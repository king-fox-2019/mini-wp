source":"src/components/userArticle.vue","name":null,"original":{"line":33,"column":0},"generated":{"line":120,"column":9}},{"source":"src/components/userArticle.vue","name":null,"original":{"line":81,"column":0},"generated":{"line":120,"column":30}},{"source":"src/components/userArticle.vue","name":null,"original":{"line":82,"column":0},"generated":{"line":121,"column":0}},{"source":"src/components/userArticle.vue","name":null,"original":{"line":82,"column":0},"generated":{"line":121,"column":9}},{"source":"src/components/userArticle.vue","name":null,"original":{"line":82,"column":0},"generated":{"line":121,"column":23}},{"source":"src/components/userArticle.vue","name":null,"original":{"line":83,"column":0},"generated":{"line":122,"column":0}},{"source":"src/components/userArticle.vue","name":null,"original":{"line":33,"column":0},"generated":{"line":123,"column":0}}],"sources":{"src/components/userArticle.vue":"<template>\n  <div>\n    <div v-if=\"profile==='profile'\">\n      <h1>Your Articles</h1>\n      <div  style=\"margin-bottom:20px;padding:20px;border-bottom: 4px solid rgba(0,0,0,.3);border-bottom-style:double\" v-for=\"(article,i) in articles\" v-bind:key=\"i\">\n      <h2>{{article.title}}</h2>\n      <div class=\"text-center\">\n        <img class=\"img-fluid\" :src=\"article.featured_image\" >\n      </div>\n      <p v-html=\"article.content\"></p>\n      <p>\n        <b>\n          <i>Author: {{article.author}}</i>\n        </b>\n      </p>\n      <p>\n        <b>\n          <i>Created: {{article.created_at}}</i>\n        </b>\n      </p>\n      <button type=\"button\" class=\"btn btn-secondary\" @click.prevent=\"editData(article)\">Edit</button>\n      <button type=\"button\" class=\"btn btn-danger\" @click.prevent=\"deleteArticle(article._id)\">Delete</button>\n    </div>\n    </div>\n    <editArticle v-if=\"profile==='edit'\" :articleData=\"edit_article\"></editArticle>\n  </div>\n</template>\n\n<script>\nimport axios from \"../../helpers/axios\";\nimport editArticle from '../components/editArticle'\n\nexport default {\n  components:{\n    editArticle\n  },\n  data() {\n    return{\n      articles: [],\n      profile:'profile',\n      edit_article:{}\n    }\n  },\n  methods: {\n    show: function(state){\n      this.profile = state\n    },\n    editData: function(article){\n      this.profile='edit'\n      console.log(this.profile)\n      this.edit_article=article\n    },\n    getUserArticle: function() {\n      axios.get('/article/user',{\n        headers:{\n          access_token:localStorage.getItem('access_token')\n        }\n      })\n      .then(articles=>{\n        for(let article of articles.data){\n          this.articles.push(article)\n        }\n      })\n      .catch(err=>console.log(err))\n    },\n    deleteArticle: function(id){\n      console.log(id)\n      axios({\n        method:'delete',\n        url:'/article',\n        data:{id},\n        headers:{access_token:localStorage.getItem('access_token')}\n      })\n      .then(result=>{\n        this.articles = []\n        this.getUserArticle()\n      })\n      .catch(err=>console.log(err))\n    }\n  },\n  created(){\n    this.getUserArticle()\n  }\n};\n</script>\n\n<style>\n</style>"},"lineCount":null}},"error":null,"hash":"c8ef7b05b06aa654a3c74d81446cd20c","cacheData":{"env":{}}}